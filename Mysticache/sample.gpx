      <groundspeak:terrain>1.5</groundspeak:terrain>
      <groundspeak:country>United States</groundspeak:country>
      <groundspeak:state>California</groundspeak:state>
      <groundspeak:short_description html="False">On your way in or out of Big Bear Lake, CA.</groundspeak:short_description>
      <groundspeak:long_description html="False">This is along our morning hike when we stay at the cabin in Big Bear.  Plenty of parking at either end of the trail, remember to have your pass if parking at 2N93.</groundspeak:long_description>
      <groundspeak:encoded_hints>eye level</groundspeak:encoded_hints>
      <groundspeak:logs>
        <groundspeak:log id="42894041">
