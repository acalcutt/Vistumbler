    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="637040" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Wedding Shower #1</groundspeak:name>
      <groundspeak:placed_by>southbayday</groundspeak:placed_by>
      <groundspeak:owner id="442337">WWC-World's Worst Cacher</groundspeak:owner>
