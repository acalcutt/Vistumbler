    <groundspeak:cache id="590905" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Where Boys Are Mean To Ducks</groundspeak:name>
      <groundspeak:placed_by>CentralCArn</groundspeak:placed_by>
      <groundspeak:owner id="768679">CentralCArn</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
      <groundspeak:container>Micro</groundspeak:container>
